#include "macros.asc"

  ORG  0x0D00

  LD   IX, BLKS
  LD   IY, BLKD
  LD   C, 0x10

LOOP:
  LD   B, (IX+0x00)
  LD   (IY+0x00), B
  INC  IX
  INC  IY
  DEC  C
  JP   NZ, LOOP

  PRINT_N_BYTE(BLKD,16)

  HALT 
  
  ORG 0x0E00

BLKD:

  ORG 0x0E80

BLKS:
  DEFB 0x00 0x01 0x02 0x03 0x04 0x05 0x06 0x07
  DEFB 0x08 0x09 0x0A 0x0B 0x0C 0x0D 0x0E 0x0F
