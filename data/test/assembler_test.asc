#include "macros.asc"

SPACE_SIZE:
  EQU  20

  ORG  0x0d00

  DEFS SPACE_SIZE

  LD   A,0x20

  PRINT_A

  LD   HL,WORK1

  PRINT_HL

  LD   A,WORK1@L

  PRINT_A

  LD   A,WORK1@H

  PRINT_A

TEMP:
  EQU  0x01

  PRINT_VALUE(TEMP)

TEMP:
  EQU  0x02

  PRINT_VALUE(TEMP)

  HALT

WORK1:
  DEFB 0x00
WORK2:
  DEFW 0x00
WORK3:
  DEFB "Hello\0"
