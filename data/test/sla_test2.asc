#include "macros.asc"

  ORG  0x1000

#define SLA(R,V) \
  LD  R,V | \
  SLA R

  SLA(A,0xED) | PRINT_A | PRINT_FLAGS
  SLA(B,0x26) | PRINT_B | PRINT_FLAGS
  SLA(C,0xED) | PRINT_C | PRINT_FLAGS
  SLA(D,0x26) | PRINT_D | PRINT_FLAGS
  SLA(E,0xED) | PRINT_E | PRINT_FLAGS
  SLA(H,0x26) | PRINT_H | PRINT_FLAGS
  SLA(L,0xED) | PRINT_L | PRINT_FLAGS

  HALT
