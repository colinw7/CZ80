#include "macros.asc"

  ORG  0x1000

#define SRA(R,V) \
  LD  R,V | \
  SRA R

  SRA(A,0xED) | PRINT_A | PRINT_FLAGS

  SRA(B,0x56) | PRINT_B | PRINT_FLAGS

  SRA(C,0xED) | PRINT_C | PRINT_FLAGS

  SRA(D,0x56) | PRINT_D | PRINT_FLAGS

  SRA(E,0xED) | PRINT_E | PRINT_FLAGS

  SRA(H,0x56) | PRINT_H | PRINT_FLAGS

  SRA(L,0xED) | PRINT_L | PRINT_FLAGS

  HALT
